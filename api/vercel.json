{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLERK_SECRET_KEY": "@clerk_secret_key",
    "GOOGLE_AI_API_KEY": "@google_ai_api_key",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "crons": []
}