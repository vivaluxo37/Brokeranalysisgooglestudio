# AI API Keys Configuration
# Copy this file to .env.local and add your actual API keys
# IMPORTANT: Never commit .env.local to version control

# Primary AI Provider - Google Gemini
VITE_API_KEY=your_gemini_api_key_here

# Fallback AI Provider 1 - HuggingFace
VITE_HUGGINGFACE_KEY=your_huggingface_api_key_here

# Fallback AI Provider 2 - OpenRouter
VITE_OPENROUTER_KEY=your_openrouter_api_key_here

# AI Provider Configuration
VITE_AI_PROVIDER_PRIORITY="gemini,huggingface,openrouter"
VITE_AI_FALLBACK_ENABLED=true
VITE_AI_RATE_LIMIT_CHECK=true
