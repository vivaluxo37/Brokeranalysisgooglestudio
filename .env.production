# Production Environment Variables for Netlify
# These are safe defaults that don't require secrets

# Frontend Configuration
VITE_API_BASE_URL=/api
VITE_APP_NAME=Broker Analysis
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AI_FEATURES=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false

# AI Provider Configuration
VITE_AI_PROVIDER_PRIORITY="proxy"
VITE_AI_FALLBACK_ENABLED=false
VITE_AI_RATE_LIMIT_CHECK=true

# Development Admin Bypass (disabled in production)
VITE_DEV_ADMIN_BYPASS=false
